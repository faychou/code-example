<view class="note-list">
    <view class="note-item" wx:for="{{noteList}}" wx:key="_id">
      <navigator url="../detail/detail?id={{item._id}}">
        <view class="note-item-title">{{item.title}}</view>
        <view class="note-item-date">{{item.time}}</view>
      </navigator>
    </view>
  </view>